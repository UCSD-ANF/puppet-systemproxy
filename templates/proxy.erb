# managed by Puppet <%= module_name %>.
# Protocols to proxy:
<% protos.each do |proto| -%>
<%= proto %><%= delim %><%= scope.lookupvar('systemproxy::proxy') %>
<% end -%>
<% if scope.lookupvar('systemproxy::no_proxy').length > 0 then %>
# Turn off proxying for these domains/subnets.
<%  no_protos.each do |proto| -%>
<%= proto %><%= delim %>"<%= no_proxy_str %>"
<%  end -%>
<% end -%>
